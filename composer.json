{
    "name": "nette/sandbox",
    "description": "The sandbox is a pre-packaged Nette Framework project, basic configured structure for your application.",
    "homepage": "https://nette.org",
    "type": "project",
    "license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
    "authors": [
        {
            "name": "David Grudl",
            "homepage": "https://davidgrudl.com"
        },
        {
            "name": "Nette Community",
            "homepage": "https://nette.org/contributors"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "nette/application": "^2.4.9",
        "nette/bootstrap": "^2.4.5",
        "nette/caching": "^2.5.6",
        "nette/di": "^2.4.10",
        "nette/finder": "^2.4.1",
        "nette/forms": "^2.4.6",
        "nette/http": "^2.4.7",
        "nette/mail": "2.4.5",
        "nette/robot-loader": "^2.4.3",
        "nette/security": "^2.4.3",
        "nette/utils": "^2.4.8",
        "latte/latte": "^2.4.8",
        "tracy/tracy": "^2.5.1",
        "dibi/dibi": "^3.2",
        "ifsnop/mysqldump-php": "2.*",
        "nextras/mail-panel": "^2.3",
        "netpromotion/profiler": "^1.3",
        "rendix2/Zebra_Mptt": "dev-master",
        "dg/composer-cleaner": "^2.0",
        "nextras/secured-links": "^1.4"
    },    
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rendix2/Zebra_Mptt"
